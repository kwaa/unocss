{
  "name": "@unocss/preset-mini",
  "type": "module",
  "version": "0.57.7",
  "description": "The minimal preset for UnoCSS",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/unocss/unocss/tree/main/packages/preset-mini#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/unocss/unocss",
    "directory": "packages/preset-mini"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./theme": {
      "import": {
        "types": "./dist/theme.d.ts",
        "default": "./dist/theme.mjs"
      },
      "require": {
        "types": "./dist/theme.d.cts",
        "default": "./dist/theme.cjs"
      }
    },
    "./variants": {
      "import": {
        "types": "./dist/variants.d.ts",
        "default": "./dist/variants.mjs"
      },
      "require": {
        "types": "./dist/variants.d.cts",
        "default": "./dist/variants.cjs"
      }
    },
    "./rules": {
      "import": {
        "types": "./dist/rules.d.ts",
        "default": "./dist/rules.mjs"
      },
      "require": {
        "types": "./dist/rules.d.cts",
        "default": "./dist/rules.cjs"
      }
    },
    "./colors": {
      "import": {
        "types": "./dist/colors.d.ts",
        "default": "./dist/colors.mjs"
      },
      "require": {
        "types": "./dist/colors.d.cts",
        "default": "./dist/colors.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.mjs"
      },
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.css",
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unocss/core": "workspace:*",
    "@unocss/extractor-arbitrary-variants": "workspace:*",
    "@unocss/rule-utils": "workspace:*"
  }
}
